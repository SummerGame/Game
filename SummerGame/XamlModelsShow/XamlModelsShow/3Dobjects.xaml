    <!--Шаблон "Обстрел"-->
    <DataTemplate x:Key="Arc">
    <Canvas>
            <Canvas.Resources>
                <PathGeometry x:Key="Arca">
                    <PathFigure StartPoint="0,0">
                        <ArcSegment Point="-280,0" Size="200,200"></ArcSegment>
                    </PathFigure>
                </PathGeometry>
            </Canvas.Resources>
            
            <Path Stroke="Yellow" StrokeThickness="2" Data="{StaticResource Arca}"
                      Canvas.Left="400" Canvas.Top="200"></Path>
            
            <Viewport3D>
                <Viewport3D.Camera>
                <PerspectiveCamera Position="1.5 1.5 1.5" LookDirection="-1 -1 -1" FieldOfView="60">
                    <PerspectiveCamera.Transform>
                        <RotateTransform3D CenterX="0.5" CenterY="0" CenterZ="0.125">
                            <RotateTransform3D.Rotation>
                                <AxisAngleRotation3D x:Name="Angle" Angle="0" Axis="0 1 0"/>
                            </RotateTransform3D.Rotation>
                        </RotateTransform3D>
                    </PerspectiveCamera.Transform>
                </PerspectiveCamera>
            </Viewport3D.Camera>
            </Viewport3D>

            <Polygon Name="Bang" IsHitTestVisible="False"
                     Points="0,12 7,20 6,8 14,11 9,3 16,0 8.5,-2 12,-5.5 4,-4 4.5,-7 0,-4.5
                     -4.5,-7 -4,-4 -12,-5.5 -8.5,-2 -16,0 -9,3 -14,11 -6,8 -7,20"
                     Fill="Yellow" Stroke="Red" StrokeThickness="2"
                     Margin="120,200,0,0" RenderTransformOrigin="0,0" Opacity="0">
                <Polygon.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform x:Name="Scale"/>
                        <RotateTransform Angle="180"/>
                        <!--<RotateTransform Angle="180"/>-->
                        <TranslateTransform/>
                    </TransformGroup>
                </Polygon.RenderTransform>
                <Polygon.Triggers>
                    <EventTrigger RoutedEvent="Window.Loaded">
                        <EventTrigger.Actions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="Bang"
                                                     Storyboard.TargetProperty="Opacity"
                                                     BeginTime="0:0:5"
                                                     Duration="0:0:0"
                                                     From="0" To="1">
                                    </DoubleAnimation>
                                    <DoubleAnimation Storyboard.TargetName="Bang"
                                                     Storyboard.TargetProperty="Opacity"
                                                     BeginTime="0:0:5.2"
                                                     Duration="0:0:0.5"
                                                     From="1" To="0">
                                    </DoubleAnimation>
                                    <DoubleAnimation Storyboard.TargetName="Scale"
                                                     Storyboard.TargetProperty="ScaleX"
                                                     BeginTime="0:0:5"
                                                     Duration="0:0:0.5"
                                                     From="1" To="5">
                                    </DoubleAnimation>
                                    <DoubleAnimation Storyboard.TargetName="Scale"
                                                     Storyboard.TargetProperty="ScaleY"
                                                     BeginTime="0:0:5"
                                                     Duration="0:0:0.5"
                                                     From="1" To="5">
                                    </DoubleAnimation>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger.Actions>
                    </EventTrigger>
                </Polygon.Triggers>

            </Polygon>
            
            <Polygon Name="CombatMarker" IsHitTestVisible="False"
                 Points="0,15 6,8 13,5 8,-3 7,-10 0,-7 -7,-10 -8,-3 -12,5 -5,8"
                 Fill="Yellow" Stroke="Red" StrokeThickness="2"
                 Opacity="1" Margin="400,200,0,0" OverridesDefaultStyle="True" RenderTransformOrigin="0,0" ScrollViewer.CanContentScroll="True">
                <Polygon.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform x:Name="Angl"/>
                        <TranslateTransform X="0" Y="0"/>
                    </TransformGroup>
                </Polygon.RenderTransform>
                <Polygon.Triggers>
                    <EventTrigger RoutedEvent="Window.Loaded">
                        <EventTrigger.Actions>
